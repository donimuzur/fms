@model FMS.Website.Models.CsfItemModel

<table id="tbCsfVehicleVendor" class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>Manufacturer</th>
            <th>Model</th>
            <th>Series</th>
            <th>Body Type</th>
            <th>Vendor</th>
            <th>Color</th>
            <th>Start Period</th>
            <th>End Period</th>
        </tr>
    </thead>
    <tbody id="tb-body-select-vehicle-from-vendor">
        @if (Model.Detail.ModelsVendor != null)
        {
            <tr>
                @Html.HiddenFor(model => model.Detail.ManufacturerVendor)
                @Html.HiddenFor(model => model.Detail.ModelsVendor)
                @Html.HiddenFor(model => model.Detail.SeriesVendor)
                @Html.HiddenFor(model => model.Detail.BodyTypeVendor)
                @Html.HiddenFor(model => model.Detail.VendorNameVendor)
                @Html.HiddenFor(model => model.Detail.ColorVendor)
                @Html.HiddenFor(model => model.Detail.StartPeriodVendor)
                @Html.HiddenFor(model => model.Detail.EndPeriodVendor)
                <td>@Html.Raw(Model.Detail.ManufacturerVendor)</td>
                <td>@Html.Raw(Model.Detail.ModelsVendor)</td>
                <td>@Html.Raw(Model.Detail.SeriesVendor)</td>
                <td>@Html.Raw(Model.Detail.BodyTypeVendor)</td>
                <td>@Html.Raw(Model.Detail.VendorNameVendor)</td>
                <td>@Html.Raw(Model.Detail.ColorVendor)</td>
                <td>@Html.Raw(Model.Detail.StartPeriodVendor.ToString("dd-MMM-yyyy"))</td>
                <td>@Html.Raw(Model.Detail.EndPeriodVendor.ToString("dd-MMM-yyyy"))</td>
            </tr>
        }
    </tbody>
</table>