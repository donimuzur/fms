@model FMS.Website.Models.TraCrfItemViewModel
<div class="panel panel-default">
    <div id="headingOne" role="tab" class="panel-heading">
        <h4 class="panel-title">
            <a aria-controls="collapseOne" aria-expanded="true" href="#collapseOne" data-parent="#accordion" data-toggle="collapse">
                Information
                <i class="fa fa-caret-down"></i>
            </a>
        </h4>
    </div>
    <div aria-labelledby="headingOne" role="tabpanel" class="panel-collapse collapse show" id="collapseOne">
        <div class="panel-body">
            <div role="form" class="form-excise">
                <div class="col-md-12">
                    <div class="box-tools pull-right" style="margin-top:-50px">
                        <h4>
                            <label for="" class="control-label">Status:</label>
                            <label class="control-label">Draft</label>
                        </h4>
                    </div>
                    <!-- general form elements -->
                    <div class="row">
                        <div class="col-md-6">
                            <div class="row">
                                <div class="col-md-12">
                                    <h5>CRF Info</h5>
                                    <hr />
                                </div>
                                <div class="col-md-12">
                                    <div class="form-group">
                                        <label class="control-label">Request Number</label>
                                        @Html.TextBoxFor(model => model.Detail.DocumentNumber, new { @class = "form-control ", @readonly = "readonly" })
                                    </div>
                                </div>
                                <div class="col-md-12">
                                    <div class="form-group">
                                        <label class="control-label">Request Date</label>
                                        @Html.TextBoxFor(model => model.Detail.CreatedDate, new { @class = "form-control ", @readonly = "readonly" })
                                    </div>

                                </div>
                                <div class="col-md-12">
                                    <div class="form-group">
                                        <label class="control-label">Coordinator</label>
                                        <input type="text" class="form-control" ng-model="coordinator" readonly="">
                                    </div>

                                </div>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="row">
                                <div class="col-md-12">
                                    <h5>Assigned Employee</h5>
                                    <hr />
                                </div>
                                <div class="col-md-12">
                                    <div class="form-group">
                                        <label for="inputEmail3" class="control-label">Employee ID</label>
                                        <input type="text" class="form-control" ng-model="employee_id" ng-click="openEmployee()" ng-disabled="roles != 'SYSTEM.ROLE_HR'" />
                                    </div>

                                </div>

                                <div class="col-md-12">
                                    <div class="form-group">
                                        <label for="" class="control-label" data-target="#employee">Employee Name</label>
                                    </div>
                                    <div class="col-md-8">
                                        <input type="text" class="form-control" ng-model="employee" readonly="" value="">
                                    </div>
                                </div>


                                <div class="col-md-12">
                                    <div class="form-group">
                                        <label for="inputEmail3" class="control-label">Cost Center</label>
                                        <input type="text" class="form-control" ng-model="cc" value="" readonly="">
                                    </div>

                                </div>
                                <div class="col-md-12">
                                    <div class="form-group">
                                        <label for="" class="control-label">New Cost Center</label>
                                        <input type="text" class="form-control" ng-model="cost_cc" ng-click="costCenter()" ng-disabled="roles != 'SYSTEM.ROLE_HR'">
                                    </div>

                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">

                        <!-- /.box-header -->
                        <div class="col-md-6">


                            <div class="row">
                                <div class="col-md-12">
                                    <h5>Vehicle Info</h5>
                                    <hr />
                                </div>
                                <div class="col-md-12">
                                    <div class="col-md-12">
                                        <div class="form-group">
                                            <label for="Vtype" class="control-label">Vehicle Type</label>
                                            <input type="text" class="form-control" value="BENEFIT" readonly>
                                        </div>
                                    </div>



                                    <div class="col-md-12">
                                        <div class="form-group">
                                            <label for="Vtype" class="control-label">Vehicle Usage</label>
                                            <input type="text" class="form-control" ng-model="vUsage" readonly>
                                        </div>

                                    </div>
                                    <div class="col-md-12">
                                        <div class="form-group">
                                            <label for="Vtype" class="control-label">Actual Group Level</label>
                                            <input type="text" class="form-control" ng-model="actual" readonly>
                                        </div>

                                    </div>


                                </div>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="row">
                                <div class="col-md-12">
                                    <h5>Location</h5>
                                    <hr />
                                </div>
                                <div class="col-md-12">
                                    <div class="col-md-12">
                                        <div class="form-group">
                                            <label for="Vtype" class="control-label">Current Location</label>
                                            <input type="text" class="form-control" readonly ng-model="kota">
                                        </div>
                                    </div>
                                    <div class="col-md-12">
                                        <div class="form-group">
                                            <label for="Vtype" class="control-label">Current Office Location</label>
                                            <input type="text" class="form-control" ng-model="alamat" value="" readonly>
                                        </div>
                                    </div>
                                    <div class="col-md-12">
                                        <div class="form-group">
                                            <label for="inputEmail3" class="control-label">New City Location</label>
                                            <input type="text" class="form-control" name="" ng-model="newkota" ng-click="openCity(2)" ng-disabled="roles != 'SYSTEM.ROLE_HR'">
                                        </div>
                                    </div>
                                    <div class="box-body">
                                        <div class="form-group">
                                            <label for="inputEmail3" class="control-label">New Office Location</label>
                                        </div>
                                        <div class="col-md-8">
                                            <input type="" class="form-control" name="" ng-model="newalamat" value="" readonly="">
                                        </div>
                                    </div>
                                    <div class="box-body">
                                        <div class="form-group">
                                            <label for="inputEmail3" class="control-label">Effective Date</label>
                                        </div>
                                        <div class="col-md-8">
                                            <div class="input-group date">
                                                <div class="input-group-addon">
                                                    <i class="fa fa-calendar">
                                                    </i>
                                                </div>
                                                <input required type="text" class="form-control pull-right" uib-datepicker-popup="dd-MMM-yyyy" close-text="Tutup" is-open="isCalendarOpened[1]" ng-model="effective_date" ng-click="openCalendar(1)" ng-disabled="roles != 'SYSTEM.ROLE_HR'">
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>





                        </div>
                    </div>
                </div>

            </div>
            <div ng-repeat="crf in dataCRF">
                <div class="col-md-12" ng-init="insertDataCRF(crf)">

                </div>
                <div class="col-md-12" ng-show="{{crf.crf_status}}>
                    4">

                </div>




                <!--     </div> -->
            </div>
        </div>

    </div>
</div>
