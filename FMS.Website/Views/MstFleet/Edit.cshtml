<script src="~/Scripts/thousand-separator.js"></script>
<script type="text/javascript">
    $(document).ready(function () {
        SetDataVehUsage();
    });

    function checkAirbag() {
        if (document.getElementById('ChkAirbag').checked == true) {
            document.getElementById('lblAirbag').innerHTML = "Yes";
        }
        else {
            document.getElementById('lblAirbag').innerHTML = "No";
        }
    }
    function checkProject() {
        if (document.getElementById('ChkProject').checked == true) {
            document.getElementById('lblProject').innerHTML = "Yes";
        }
        else {
            document.getElementById('lblProject').innerHTML = "No";
        }
    }
    function checkRestitution() {
        if (document.getElementById('ChkRestitution').checked == true) {
            document.getElementById('lblRestitution').innerHTML = "Yes";
            calculateVAT();
        }
        else {
            document.getElementById('lblRestitution').innerHTML = "No";
            $('#VatDecimal').val(0);
        }
    }
    function IsAngka(evt) {
        var charCode = (evt.which);
        if (charCode > 31 && (charCode < 48 || charCode > 57)) {
            return false;
        }
        return true;

    }
    $('#VatDecimalStr').val(ThausandSeperator($('#VatDecimalStr').val(), 2));
    $('#MonthlyHMSInstallmentStr').val(ThausandSeperator($('#MonthlyHMSInstallmentStr').val(), 2));
    $('#TotalMonthlyChargeStr').val(ThausandSeperator($('#TotalMonthlyChargeStr').val(), 2));

    function calculateVAT() {
        $('#VatDecimal').val($('MonthlyHMSInstallment').val()*10/100);
        $('#VatDecimal').val(ThausandSeperator($('#VatDecimal').val(), 2));
    }

    function ajaxSelectVehUsage(urlFunction, vehType, vehUsage) {
        $.ajax({
            type: 'POST',
            url: urlFunction,
            data: { vehType: vehType },
            success: function (data) {
                for (var i = 0; i < data.length; i++) {
                    $('#VehicleUsage').append('<option value="' + data[i].SettingValue + '">' + data[i].SettingValue + '</option>');
                }

                $('#VehicleUsage').val(vehUsage);
            }
        });
    }

    $("#VehicleType").change(function () {
        $('#VehicleUsage').html('');
        $('#VehicleUsage').append('<option value="">Select</option>');
        if ($("#VehicleType").length) {
            ajaxSelectVehUsage('@Url.Action("GetVehUsageByVehType", "MstFleet")', $(this).val(), '');
        }
    });

    function SetDataVehUsage() {
        var vehType = '@Model.VehicleType';
        var vehUsage = '@Model.VehicleUsage';

        $('#VehicleUsage').html('');
        $('#VehicleUsage').append('<option value="">Select</option>');
        ajaxSelectVehUsage('@Url.Action("GetVehUsageByVehType", "MstFleet")', vehType, vehUsage);
    }

    jQuery(function ($) {
        if ($('.title-page').html() == 'Master Data Fleet') {
            $('#MstFleetMenu').addClass('active');
        }
    });
</script>