                <td>@Html.Raw(data.FuelType)</td>
                <td>@Html.Raw(data.Branding)</td>
                <td>@Html.Raw(data.Colour)</td>
                <td>@Html.Raw(data.Airbag ==false ? "No" : "Yes" )</td>
                <td>@Html.Raw(data.VehicleYear.HasValue ? data.VehicleYear.ToString() : "")</td>
                <td>@Html.Raw(data.VehicleType)</td>
                <td>@Html.Raw(data.VehicleUsage)</td>
                <td>@Html.Raw(data.Project == true ? "Yes" : "No")</td>
                <td>@Html.Raw(data.ProjectName)</td>
                <td>@Html.Raw(data.StartContract == null ? "" : data.StartContract.Value.ToString("dd-MMM-yyyy"))</td>
                <td>@Html.Raw(data.EndContract == null ? "" : data.EndContract.Value.ToString("dd-MMM-yyyy"))</td>
                <td>@Html.Raw(data.Vendor)</td>
                <td>@Html.Raw(data.City)</td>
                <td>@Html.Raw(data.SupplyMethod)</td>
                <td>@Html.Raw(data.Restitution == true ? "Yes" : "No")</td>
                <td>@Html.Raw(data.MonthlyInstallment)</td>
                <td>@Html.Raw(data.Vat)</td>
                <td>@Html.Raw(data.PoNumber)</td>
                <td>@Html.Raw(data.PoLine)</td>
                <td>@Html.Raw(data.carGrouplevel)</td>
                <td>@Html.Raw(data.EmployeeGroupLevel)</td>
                <td>@Html.Raw(data.AssignedTo)</td>
                <td>@Html.Raw(data.Address)</td>
                <td>@Html.Raw(data.StartDate == null ? "" : data.StartDate.Value.ToString("dd-MMM-yyyy"))</td>
                <td>@Html.Raw(data.TerminationDate == null ? "" : data.TerminationDate.Value.ToString("dd-MMM-yyyy"))</td>
                <td>@Html.Raw(data.VehicleStatus == false ? "InActive" : "Active")</td>
                <td>@Html.Raw(data.CertificateOfOwnership)</td>
                <td>@Html.Raw(data.Comments)</td>
