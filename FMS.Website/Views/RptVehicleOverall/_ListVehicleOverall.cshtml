        </tr>
    </thead>
    <tbody>
        @foreach (var data in Model.ListVehicle)
        {
            <tr>
                <td>@Html.Raw(data.PoliceNumber)</td>
                <td>@Html.Raw(data.EmployeeId)</td>
                <td>@Html.Raw(data.EmployeeName)</td>
                <td>@Html.Raw(data.CostCenter)</td>
                <td>@Html.Raw(data.Manufacture)</td>
                <td>@Html.Raw(data.Models)</td>
                <td>@Html.Raw(data.Series)</td>
                <td>@Html.Raw(data.Transmission)</td>
                <td>@Html.Raw(data.BodyType)</td>
                <td>@Html.Raw(data.FuelType)</td>
                <td>@Html.Raw(data.Branding)</td>
                <td>@Html.Raw(data.Colour)</td>
                <td>@Html.Raw(data.Airbag ==false ? "No" : "Yes" )</td>
                <td>@Html.Raw(data.Abs == false ? "No" : "Yes")</td>
                <td>@Html.Raw(data.ChasisNumber)</td>
                <td>@Html.Raw(data.EngineNumber)</td>
                <td>@Html.Raw(data.VehicleStatus == false ? "InActive" : "Active")</td>
                <td>@Html.Raw(data.StartContract == null ? "" : data.StartContract.Value.ToString("yyyy-MM-dd"))</td>
                <td>@Html.Raw(data.EndContract == null ? "" : data.EndContract.Value.ToString("yyyy-MM-dd"))</td>
                <td>@Html.Raw(data.Vendor)</td>
                <td>@Html.Raw(data.AssetsNumber)</td>
                <td>@Html.Raw(data.City)</td>
                <td>@Html.Raw(data.SupplyMethod)</td>
                <td>@Html.Raw(data.TerminationDate == null ? "" : data.TerminationDate.Value.ToString("yyyy-MM-dd"))</td>
                <td>@Html.Raw(data.MonthlyInstallment)</td>
                <td>@Html.Raw(data.Vat)</td>
                <td>@Html.Raw(data.TotalMonthlyInstallment)</td>
                <td>@Html.Raw(data.PoNumber)</td>
                <td>@Html.Raw(data.PoLine)</td>
                <td>@Html.Raw(data.Function)</td>
                <td>@Html.Raw(data.Regional)</td>
            </tr>
        }
    </tbody>
</table>